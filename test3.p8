pico-8 cartridge // http://www.pico-8.com
version 27
__lua__
function _init()
	p={
		x=64,
		y=64,
		turn=false
	}

end

function _update()

button_triggered=false

playerloc = mget(p.x/8, p.y/8)

if playerloc == 2 then
	if(button_triggered != true) then
		button_triggered=true
		sfx(1)
	end
else
	button_triggered=false	
end

if btn(1) then
	p.x+=1

end

if btn(0) then
	p.x-=1

end
if btn(3) then
	p.y+=1

	end
	
if btn(2) then
	p.y-=1
	end
	
		

end

function _draw()

cls()


local ps
ps=1

print (bro)


map(0,0,0,0,128,128)

spr(ps,p.x,p.y)

end
